<%- include("partials/header"); -%>

<% if (users.length === 0) { %>
	<p><em>No users found!</em></p>
<% } else { %>
	<ul>
	<% for (let user of users) { %>
		<li>
			<strong><a href="<%= `/users/${user._id}/` %>"><%= user.email %> (<%= user._id %>)</a></strong>
		</li>
	<% } %>
	</ul>
<% } %>

<form action="/users/" class="pure-form">
	<p>
		<textarea name="mq" class="pure-input-1"><%= queryString %></textarea>
	</p>
	<p>
		<input type="submit" class="pure-button" value="Search Again" />
	</p>
</form>

<%- include("partials/footer"); -%>
